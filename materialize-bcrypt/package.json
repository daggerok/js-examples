  "license": "MIT",
  "devDependencies": {
    "@types/node": "10.0.4",
    "@types/requirejs": "2.1.31",
    "@types/webpack": "4.1.4",
    "@types/whatwg-fetch": "0.0.33",
    "autoprefixer": "8.4.1",
    "base-href-webpack-plugin": "2.0.0",
    "cross-env": "5.1.4",
    "css-loader": "0.28.11",
    "eslint": "4.19.1",
    "file-loader": "1.1.11",
    "gh-pages": "1.1.0",
    "google-fonts-webpack-plugin": "0.4.4",
    "html-webpack-plugin": "3.2.0",
    "lite-server": "2.3.0",
    "mini-css-extract-plugin": "0.4.0",
    "ncp": "2.0.0",
    "node-sass": "4.9.0",
    "npm-run-all": "4.1.3",
    "optimize-css-assets-webpack-plugin": "4.0.1",
    "postcss-loader": "2.1.5",
    "precss": "3.1.2",
    "rimraf": "2.6.2",
    "sass-loader": "7.0.1",
    "style-loader": "0.21.0",
    "uglifyjs-webpack-plugin": "1.2.5",
    "url-loader": "1.0.1",
    "webpack": "4.7.0",
    "webpack-cli": "2.1.3"
  },
  "scripts": {
    "rimraf": "rimraf -rf ./dist",
    "eslint": "eslint ./app",
    "lint": "yarn eslint",
    "webpack": "cross-env NODE_ENV=development webpack",
    "lite-server": "lite-server -c ./lite-server.config.js",
    "prebuild": "yarn rimraf",
    "build": "cross-env NODE_ENV=production webpack",
    "preserve": "yarn build",
    "serve": "yarn lite-server",
    "prestart": "yarn webpack",
    "start": "npm-run-all -p start:*",
    "start:lite-server": "yarn lite-server",
    "start:webpack-watch": "yarn webpack --watch",
    "predeploy": "yarn -i && cross-env npm-run-all predeploy:*",
    "predeploy:rimraf": "yarn rimraf",
    "predeploy:eslint": "eslint app",
    "predeploy:webpack": "cross-env NODE_ENV=gh-pages GHPAGES=true webpack",
    "predeploy:gh-pages-404": "ncp ./dist/index.html ./dist/404.html",
    "predeploy:nojekyll": "touch ./dist/.nojekyll",
    "deploy": "gh-pages -e ./ -d ./dist -s '{*.*,.*,*,**}' --branch gh-pages --dotfiles=true -m \"Github Pages $(date +%Y-%m-%d) deployment\"",
    "postdeploy": "echo '(;'"
  },
  "homepage": "https://daggerok.github.io/js-examples",
  "dependencies": {
    "bcryptjs": "2.4.3",
    "materialize-css": "1.0.0"
  }
}
