  "devDependencies": {
    "@types/node": "9.6.5",
    "@types/requirejs": "2.1.31",
    "@types/webpack": "4.1.3",
    "@types/whatwg-fetch": "0.0.33",
    "base-href-webpack-plugin": "2.0.0",
    "cross-env": "5.1.4",
    "css-loader": "0.28.11",
    "eslint": "4.19.1",
    "gh-pages": "1.1.0",
    "html-webpack-plugin": "3.2.0",
    "lite-server": "2.3.0",
    "ncp": "2.0.0",
